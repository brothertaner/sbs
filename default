server {
    listen 80;
    server_name oopsnet.shop;

    include /etc/nginx/users/80/*.conf;
}

server {
    listen 443 ssl http2;
    server_name oopsnet.shop;

    ssl_certificate /etc/letsencrypt/live/oopsnet.shop/fullchain.pem;
    ssl_certificate_key /etc/letsencrypt/live/oopsnet.shop/privkey.pem;

    include /etc/nginx/users/443/*.conf;
}
